{
  "author": "Antoine Cotten",
  "license": "Apache-2.0",
  "name": "aco-yum_autoupdate",
  "project_page": "https://github.com/tOnI0/aco-yum_autoupdate",
  "source": "git://github.com/tOnI0/aco-yum_autoupdate.git",
  "summary": "Puppet module for scheduled system updates on RHEL variants",
  "version": "0.6.0",
  "tags": ["unattended","update","yum"],
  "dependencies": [
    {
      "name": "puppetlabs/stdlib",
      "version_requirement": ">= 1.0.0 < 5.0.0"
    }
  ],
  "operatingsystem_support": [
    {
      "operatingsystem": "RedHat",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "CentOS",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "OracleLinux",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Scientific",
      "operatingsystemrelease": [
        "5",
        "6",
        "7"
      ]
    },
    {
      "operatingsystem": "Fedora",
      "operatingsystemrelease": [
        "19",
        "20",
        "21"
      ]
    }
  ],
  "requirements": [
    {
      "name": "pe",
      "version_requirement": ">= 3.0.0"
    },
    {
      "name": "puppet",
      "version_requirement": ">= 2.7.0"
    }
  ]
}
